{
  "expo": {
    "scheme": "Clanforge",
    "name": "Clanforge",
    "slug": "Clanforge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LightLogo.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/LightLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#EFF2F7"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LightLogo.png",
        "backgroundColor": "#EFF2F7"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/LightLogo.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
